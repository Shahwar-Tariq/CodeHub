@model IEnumerable<CodeHub.Models.course>
  @{
      if (Session["role"].ToString() == "Expert")
      {
          Layout = "~/Views/Shared/_ExpertLayout.cshtml";
      }
      else
      {
          Layout = "~/Views/Shared/_Layout.cshtml";

      }
  }

