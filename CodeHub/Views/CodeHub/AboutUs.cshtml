@model CodeHub.ViewModel.AboutUsViewModel
@{
    ViewBag.Title = "AboutUs";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}

<body class="page body_style_fullscreen body_filled article_style_boxed top_panel_style_dark top_panel_opacity_solid top_panel_above menu_right sidebar_hide">
    <a id="toc_top" class="sc_anchor" title="To Top" data-description="&lt;i&gt;Back to top&lt;/i&gt; - &lt;br&gt;scroll to top of the page" data-icon="icon-angle-double-up" data-url="" data-separator="yes"></a>
    <!-- Body -->
    <div class="body_wrap">
        <div class="page_wrap">
            <div class="top_panel_fixed_wrap"></div>
            <header class="top_panel_wrap bg_tint_dark">
                <!-- User menu -->
                <!-- /User menu -->
                <!-- Main menu -->
                <!-- /Main menu -->
            </header>
            <!-- Page title -->
            <div class="page_top_wrap page_top_title page_top_breadcrumbs sc_pt_st1">
                <div class="content_wrap">
                    <div class="breadcrumbs">
                        <a class="breadcrumbs_item home" href="/CodeHub/Index">Home</a>
                        <span class="breadcrumbs_delimiter"></span>
                        <span class="breadcrumbs_item current">About Us</span>
                    </div>
                    <h1 class="page_title">About Us</h1>
                </div>
            </div>
            <!-- /Page title -->
            <!-- Content without sidebar -->
            <div class="page_content_wrap">
                @if (TempData["message"] != null)
                {
                    <div class="w-lg-600px p-10 p-lg-15 mx-auto" style="width:500px">
                        @Html.Raw(@TempData["message"])
                    </div>
                }
                <div class="content">
                    <article class="post_item post_item_single page">
                        <section class="post_content">
                            <!-- Info section -->
                            <div class="sc_section accent_top bg_tint_light sc_bg1" data-animation="animated fadeInUp normal">
                                <div class="sc_section_overlay">
                                    <div class="sc_section_content">
                                        <div class="sc_content content_wrap margin_top_2_5em_imp margin_bottom_2_5em_imp">
                                            <h2 class="sc_title sc_title_regular sc_align_center margin_top_0 margin_bottom_085em text_center">Why choose online education?</h2>
                                            <div class="columns_wrap sc_columns columns_nofluid sc_columns_count_2">
                                                <div class="column-1_2 sc_column_item sc_column_item_1 sc_info_st1 odd first ">
                                                    <div class="sc_section font_1_25em lh_1_3em">
                                                        <span class="sc_icon icon-pen-2 alignleft link_color disp_block margin_right_05em margin_bottom_015em font_3_5em lh_1em"></span>
                                                        <p>
                                                            Study, have fun, uncover a new passion or learn
                                                            <br /> skills that just may change your life.
                                                        </p>
                                                    </div>
                                                    <div class="sc_section font_1_25em lh_1_3em margin_top_05em_imp">
                                                        <span class="sc_icon icon-new-2 alignleft link_color disp_block margin_right_05em margin_bottom_015em font_3_5em lh_1em"></span>
                                                        <p>
                                                            Become an online Education student, it&#8217;s easy&#8230;
                                                            <br /> Take online Education courses at your pace, at home or in a cafe.
                                                        </p>
                                                    </div>
                                                    <div class="sc_section font_1_25em lh_1_3em margin_top_05em_imp">
                                                        <span class="sc_icon icon-lightbulb-2 alignleft link_color disp_block margin_right_05em margin_bottom_015em font_3_5em lh_1em"></span>
                                                        <p>
                                                            From our think tank to your screen—we help you
                                                            <br /> learn through videos and text tutorials.
                                                        </p>
                                                    </div>
                                                </div>
                                                <div class="column-1_2 sc_column_item sc_column_item_2 sc_info_st1 even">
                                                    <div class="sc_video_player">
                                                        <div class="sc_video_frame width_596 height_335" data-width="596" data-height="335">
                                                            <iframe class="video_frame" src="http://player.vimeo.com/video/9679622" width="596" height="335"></iframe>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /Info section -->
                            <!-- Team section -->
                            <div class="sc_section" data-animation="animated fadeInUp normal">
                                <div class="sc_content content_wrap margin_top_3em_imp margin_bottom_3em_imp">
                                    <h2 class="sc_title sc_title_regular sc_align_center text_center margin_top_0 margin_bottom_085em" style="font-weight:400" 5>Our Experts</h2>
                                    <div class="sc_team sc_team_style_1">
                                        <div class="sc_columns columns_wrap">
                                            @foreach (var e in @Model.uzr)
                                            {
                                                <div class="column-1_3">
                                                    <div class="sc_team_item sc_team_item_1 odd first">
                                                        @if (e.image_path != null)
                                                        {
                                                            <div class="sc_team_item_avatar">
                                                                <img alt="" src="~/Images/profile_images/@e.image_path" style="height:250px;width:343.9px">
                                                            </div>
                                                        }
                                                        else
                                                        {
                                                            <div class="sc_team_item_avatar">
                                                                <img alt="" src="~/assets/images/user-windows-10-user-icon-png-transparent-png.png" style="height:250px;width:343.9px">
                                                            </div>
                                                        }
                                                        <div class="sc_team_item_info">
                                                            <h6 class="sc_team_item_title">
                                                                <a @*href="/Expert/PersonalPage?id=@e.user_id"*@>@e.first_name @e.last_name</a>
                                                            </h6>
                                                            <div class="sc_team_item_position">@e.skills</div>


                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /Team section -->
                            <!-- Courses section -->
                            <div class="sc_section accent_top bg_tint_light sc_bg1" data-animation="animated fadeInUp normal">
                                <div class="sc_section_overlay">
                                    <div class="sc_section_content">
                                        <div class="sc_content content_wrap margin_top_2_5em_imp margin_bottom_2_5em_imp">
                                            <h2 class="sc_title sc_title_regular sc_align_center margin_top_0 margin_bottom_085em text_center" style="font-weight:400">..Courses..</h2>
                                            <div class="sc_blogger layout_courses_3 template_portfolio sc_blogger_horizontal no_description">
                                                <div class="isotope_wrap" data-columns="3">

                                                    @foreach (var data in @Model.corse)
                                                    {

                                                        <div class="isotope_item isotope_item_courses isotope_column_3 flt_57">
                                                            <article class="post_item post_item_courses odd">
                                                                <div class="post_content isotope_item_content ih-item colored square effect_dir left_to_right">
                                                                    <div class="post_featured img">

                                                                        <a href="/Courses/GetCourse?course_code=@data.course_code">
                                                                            <img alt="" src="~/Images/Course_Images/@data.image" style="height:350px; width:400px">
                                                                        </a>
                                                                        <h4 class="post_title">
                                                                            <a href="/Courses/GetCourse?course_code=@data.course_code">@data.course_name</a>
                                                                        </h4>

                                                                        <div class="post_descr">
                                                                            <div class="post_price">
                                                                                <span class="post_price_value">@data.type</span>
                                                                            </div>

                                                                            <div class="post_rating reviews_summary blog_reviews">
                                                                                <div class="criteria_summary criteria_row">

                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="post_info_wrap info">

                                                                        <div class="info-back">
                                                                            <h4 class="post_title">
                                                                                <a href="/Courses/GetCourse?course_code=@data.course_code">@data.course_name</a>
                                                                            </h4>
                                                                            <div class="post_descr">
                                                                                <p>
                                                                                    <a href="/Courses/GetCourse?course_code=@data.course_code">@Html.Raw(@data.summary)</a>
                                                                                </p>
                                                                                <div class="post_buttons">
                                                                                    <div class="post_button">
                                                                                        <a href="/Courses/GetCourse?course_code=@data.course_code" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">LEARN MORE</a>
                                                                                    </div>
                                                                                    @if (@data.type == "Paid")
                                                                                    {
                                                                                        if (Session["id"] != null)
                                                                                        {

                                                                                            if (Model.cartitem.Count() != 0)
                                                                                            {

                                                                                                if (Model.cartitem.FirstOrDefault(x => x.course_code == data.course_code) != null)
                                                                                                {
                                                                                                    <div class="post_button">
                                                                                                        <a href="/CodeHub/ViewCart" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">In Cart</a>
                                                                                                    </div>

                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="post_button">
                                                                                                        <a href="/CodeHub/BuyCourse?course_code=@data.course_code" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">BUY NOW</a>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                            else
                                                                                            {

                                                                                                if (Model.ord.Count() == 0)
                                                                                                {
                                                                                                    <div class="post_button">
                                                                                                        <a href="/CodeHub/BuyCourse?course_code=@data.course_code" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">BUY NOW</a>
                                                                                                    </div>

                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    if (Model.orddet.FirstOrDefault(x => x.course_code == @data.course_code) != null)
                                                                                                    {
                                                                                                        var id = Session["id"].ToString();
                                                                                                        var userid = Model.ord.FirstOrDefault().user_id.ToString();
                                                                                                        var ordr_detl = Model.ord.FirstOrDefault().order_details.FirstOrDefault().course_code;
                                                                                                        var orderid = Model.orddet.FirstOrDefault(x => x.course_code == data.course_code).order_id;
                                                                                                        if (Model.ord.FirstOrDefault(x => x.id == orderid).status == "COMPLETED")
                                                                                                        {
                                                                                                            <div>

                                                                                                            </div>
                                                                                                        }
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="post_button">
                                                                                                            <a href="/CodeHub/BuyCourse?course_code=@data.course_code" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">BUY NOW</a>
                                                                                                        </div>
                                                                                                    }

                                                                                                }

                                                                                            }

                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            <div class="post_button">
                                                                                                <a href="/CodeHub/BuyCourse?course_code=@data.course_code" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_link sc_button_size_small">BUY NOW</a>
                                                                                            </div>
                                                                                        }


                                                                                    }
                                                                                </div>
                                                                            </div>
                                                                        </div>


                                                                    </div>
                                                                </div>
                                                            </article>
                                                        </div>
                                                    }
                                                </div>
                                            </div>
                                            <a href="/Courses/Index" class="sc_button sc_button_square sc_button_style_filled sc_button_bg_menu sc_button_size_small aligncenter sc_button_iconed icon-graduation margin_top_1em margin_bottom_4 widht_12em">VIEW ALL COURSES</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        
                        </section>
                    </article>
                </div>
            </div>
          
        </div>
    </div>
    <!-- /Body -->

</body>
