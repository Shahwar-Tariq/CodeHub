@model IEnumerable<CodeHub.Models.feedback>
@{
    ViewBag.Title = "Feedback";
    if (Session["role"].ToString() == "Expert")
    {
        Layout = "~/Views/Shared/_ExpertLayout.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";

    }
}

<div class="toolbar">
    <!--begin::Toolbar-->
    <div class="container-fluid py-6 py-lg-0 d-flex flex-column flex-sm-row align-items-lg-stretch justify-content-sm-between">
        <!--begin::Page title-->
        <div class="page-title d-flex flex-column me-5">
            <!--begin::Title-->
            <h1 class="d-flex flex-column text-dark fw-bolder fs-2 mb-0">Feedback</h1>
            <!--end::Title-->
            <!--begin::Breadcrumb-->
            <ul class="breadcrumb breadcrumb-separatorless fw-bold fs-7 pt-1">
                <!--begin::Item-->
                <li class="breadcrumb-item text-muted">
                    <a href="/Admin/Index" class="text-muted text-hover-primary">Home</a>
                </li>
                <!--end::Item-->
                <!--begin::Item-->
                <li class="breadcrumb-item">
                    <span class="bullet bg-gray-300 w-5px h-2px"></span>
                </li>
                <!--end::Item-->
                <!--begin::Item-->
                <li class="breadcrumb-item text-muted">Feedbacks</li>
                <!--end::Item-->
                <!--begin::Item-->
                <li class="breadcrumb-item">
                    <span class="bullet bg-gray-300 w-5px h-2px"></span>
                </li>
                <!--end::Item-->
                <!--begin::Item-->
                <li class="breadcrumb-item text-dark">View Feedback</li>
                <!--end::Item-->
            </ul>
            <!--end::Breadcrumb-->
        </div>
        <!--end::Page title-->
        <!--begin::Action group-->
  
        <!--end::Action group-->
    </div>
    <!--end::Toolbar-->
</div>

@if (TempData["message"] != null)
{
    <div class="w-lg-600px p-10 p-lg-15 mx-auto" style="width:500px">
        @Html.Raw(@TempData["message"])
    </div>
}
<div class="card-body"  style="margin-top:45px; padding-left:45px; padding-right:45px">
    <!--begin::Title-->
  
    <!--end::Title-->
