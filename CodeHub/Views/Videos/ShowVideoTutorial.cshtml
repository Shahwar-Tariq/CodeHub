@model CodeHub.ViewModel.TopicContentViewModel
@{
    ViewBag.Title = "ShowVideoTutorial";
    Layout = "~/Views/Shared/NoLayout.cshtml";
}
<!DOCTYPE html>
<html lang="en-US">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="apple-touch-icon" sizes="180x180" href="/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon/favicon-16x16.png">
    <link rel="manifest" href="/favicon/site.webmanifest">
    <title>Code Hub</title>

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans%3A300italic%2C400italic%2C600italic%2C300%2C400%2C600&#038;subset=latin%2Clatin-ext&#038;ver=4.3.1" type="text/css" media="all" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:100,100italic,300,300italic,400,400italic,700,700italic&#038;subset=latin,latin-ext,cyrillic,cyrillic-ext" type="text/css" media="all" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Love+Ya+Like+A+Sister:400&#038;subset=latin" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/fontello/css/fontello.css" type="text/css" media="all" />

    <link rel="stylesheet" href="/assets/js/rs-plugin/settings.css" type="text/css" media="all" />

    <link rel="stylesheet" href="/assets/css/woocommerce/woocommerce-layout.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/woocommerce/woocommerce-smallscreen.css" type="text/css" media="only screen and (max-width: 768px)" />
    <link rel="stylesheet" href="/assets/css/woocommerce/woocommerce.css" type="text/css" media="all" />

    <link rel="stylesheet" href="/assets/css/style.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/shortcodes.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/core.animation.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/tribe-style.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/skins/skin.css" type="text/css" media="all" />

    <link rel="stylesheet" href="/assets/css/core.portfolio.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/mediaelement/mediaelementplayer.min.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/mediaelement/wp-mediaelement.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/prettyPhoto/css/prettyPhoto.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/core.customizer/front.customizer.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/core.messages/core.messages.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/js/swiper/idangerous.swiper.min.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/custom-style.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/responsive.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/assets/css/skins/skin-responsive.css" type="text/css" media="all" />
    <script src="https://www.paypal.com/sdk/js?client-id=AVb-t6NwOKCF_bCxtmgEN8iFoEx29crkYuLqiXNr2V6r_7YQ4Q5y3ekfIzPCrktsqo6DEYgWV0K8vyfO&currency=USD"></script>
</head>

<body class="single postid-636 single-courses body_style_fullscreen body_filled article_style_stretch layout_single-courses top_panel_style_dark top_panel_opacity_solid top_panel_above menu_right sidebar_hide">
    <a id="toc_top" class="sc_anchor" title="To Top" data-description="&lt;i&gt;Back to top&lt;/i&gt; - &lt;br&gt;scroll to top of the page" data-icon="icon-angle-double-up" data-url="" data-separator="yes"></a>
    <!-- Body -->
    <div class="body_wrap">
        <div class="page_wrap">
            <div class="top_panel_fixed_wrap"></div>
            <header class="top_panel_wrap bg_tint_dark">
                <!-- User menu -->
                <div class="menu_user_wrap">
                    <div class="content_wrap clearfix">
                        <div class="menu_user_area menu_user_right menu_user_nav_area">
                            <ul id="menu_user" class="menu_user_nav">
                                @*<li class="menu_user_bookmarks">
                                        <a href="#" class="bookmarks_show icon-star-1" title="Show bookmarks"></a>
                                        <ul class="bookmarks_list">
                                            <li><a href="#" class="bookmarks_add icon-star-empty" title="Add the current page into bookmarks">Add bookmark</a></li>
                                        </ul>
                                    </li>*@
                                <li class="menu_user_controls">
                                    @if (Session["id"] != null)
                                    {
                                        if (Session["role"].ToString() == "Expert")
                                        {
                                            <a href="/Expert/Index">
                                                <span class="user_name">Dashboard</span>
                                            </a>
                                        }
                                        else if (Session["role"].ToString() == "Admin")
                                        {
                                            <a href="/Admin/Index">
                                                <span class="user_name">Dashboard</span>
                                            </a>
                                        }
                                        else
                                        {
                                            <a href="/CodeHub/Dashboard">
                                                <span class="user_name">Dashboard</span>
                                            </a>
                                        }
                                    }
                                </li>

                                <li class="menu_user_logout">
                                    @if (Session["id"] != null)
                                    {
                                        <a href="/Authentication/Logout" class="icon icon-logout">Logout</a>
                                    }
                                    else
                                    {
                                        <a href="/Authentication/Login" class="icon icon-logout">SignIn</a>
                                    }

                                </li>
                            </ul>
                        </div>

                    </div>
                </div>
                <!-- /User menu -->
                <!-- Main menu -->
                <div class="menu_main_wrap logo_left">
                    <div class="content_wrap clearfix">
                        <!-- Logo -->
                        <div class="logo" style="margin-top:0px">
                            <a href="/CodeHub/Index">
                                <img src="~/assets/media/avatars/Logooe-removebg-preview.png" alt="logo_main" style="height:80px;width:100%; margin-left:30px;" />
                            </a>


                        </div>
                        <!-- Logo -->
                        <!-- Search -->
                        <div class="search_wrap search_style_regular search_ajax" title="Open/close search form">
                            <a href="#" class="search_icon icon-search-2"></a>
                            <div class="search_form_wrap">
                                <form method="get" class="search_form" action="#">
                                    <button type="submit" class="search_submit icon-zoom-1" title="Start search"></button>
                                    <input type="text" class="search_field" placeholder="" value="" name="s" title="" />
                                </form>
                            </div>
                            <div class="search_results widget_area bg_tint_light">
                                <a class="search_results_close icon-delete-2" onclick="myFUNCTION()"></a>
                                <div class="search_results_content">
                                </div>
                            </div>
                        </div>
                        <!-- /Search -->
                        <!-- Navigation -->
                        <a href="#" class="menu_main_responsive_button icon-menu-1"></a>
                        <nav class="menu_main_nav_area" style="display:inline" id="myDIV">
                            <ul id="menu_main" class="menu_main_nav">
                                <li class="menu-item menu-item-has-children ">
                                    <a href="/CodeHub/Index">Homepage</a>

                                </li>
                                <li class="menu-item menu-item-has-children ">
                                    <a href="/Courses/GetCourse?course_code=@Model.crs.course_code">Courses</a>

                                </li>
                                <li class="menu-item menu-item-has-children">
                                    <a href="/Expert/TeamMembers">Experts</a>
                                    <ul class="sub-menu">
                                        <li class="menu-item"><a href="/Expert/TeamMembers">Expert Team</a></li>

                                        <li class="menu-item"><a href="/Expert/RequestForJoinAsExpert">Request to Join</a></li>
                                    </ul>
                                </li>
                                <li class="menu-item menu-item-has-children">
                                    <a href="/Videos/ShowVideoTutorial?course_code=@Model.crs.course_code">Video Tutorials</a>

                                </li>
                                <li class="menu-item menu-item-has-children">
                                    <a href="/Quiz/ReadQuiz?course_code=@Model.crs.course_code">Quizes</a>

                                </li>
                                <li class="menu-item menu-item-has-children ">
                                    <a href="/CodeHub/Compiler">Compiler</a>

                                </li>


                            </ul>
                        </nav>
                        <!-- /Navigation -->
                    </div>
                </div>
                <!-- /Main menu -->
            </header>

            <div class="page_content_wrap">
                <!-- Course info section -->
                <div class="content">
                    @if (TempData["message"] != null)
                    {
                        <div class="w-lg-600px p-10 p-lg-15 mx-auto" style="width:500px">
                            @Html.Raw(@TempData["message"])
                        </div>
                    }
                    <article class="post_item post_item_single_courses courses" style="padding-left:0px">
                        <!-- Sidebar -->
                        <div class="sidebar widget_area bg_tint_light sidebar_style_light" style="float:left;width:20%;margin-left:0px;overflow:auto;height:630px">

                            <!-- Categories widget -->
                            <aside class="widget">

                                <h5 class="widget_title">@Model.crs.course_name Tutorial</h5>
                                <ul style="margin-left:20px;">
                                    @foreach (var data in @Model.tpc)
                                    {
                                        <li> <a href="/Videos/ShowVideoTutorial?topic_id=@data.id&course_code=@data.course_code"> @data.topic_name </a> </li>
                                    }

                                </ul>



                            </aside>
                            <!-- /Categories widget -->

                        </div>
                        <!-- /Sidebar -->
                        <div class="content_wrap" style="float:right;width:70%">

                            <section class="post_content">
                                <!-- Course info section -->
                                <div class="sc_reviews alignright" style="visibility:hidden">
                                    <div class="reviews_block">
                                        <div id="users_marks" class="sc_tabs_content disp_block">
                                            <div class="reviews_editor">
                                                <div class="reviews_item reviews_max_level_100" data-max-level="100" data-step="1">

                                                </div>
                                                <div class="reviews_item reviews_max_level_100" data-max-level="100" data-step="1">

                                                </div>
                                                <div class="reviews_item reviews_max_level_100" data-max-level="100" data-step="1">

                                                </div>
                                                <div class="reviews_item reviews_max_level_100" data-max-level="100" data-step="1">

                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                @*   <h2 class="margin_top_2em">@Model.crs.course_name.FirstOrDefault()</h2>*@




                                <br />
                                @if (@Model.crs.type == "Paid")
                                {

                                    if (Session["id"] != null && Session["role"].ToString() == "Student" )
                                    {
                                        if (Model.ordrr != null && Model.ordrr.Count > 0)
                                        {
                                            var id = Session["id"].ToString();
                                            var orduserdid = Model.ordrr.FirstOrDefault().user_id.ToString();
                                            if (Model.ordrr.FirstOrDefault().status == "COMPLETED" && orduserdid == id)
                                            {
                                                <div style="width:60%">
                                                    @if (Model.cnt != null)
                                                    {
                                                     
                                                        <div class="video-frame">
                                                            <video style="width:100%; height:500px;" controls>
                                                                <source src="~/VideoFileUpload/@Model.cnt.videoname" type="video/mp4" />
                                                            </video>
                                                        </div><br />
                                                        @*<button>Previous</button>
                                                            <button style="margin-left:600px">Next</button>*@
                                                    }
                                                </div>

                                                <div class="columns_wrap sc_columns columns_nofluid sc_columns_count_3">
                                                    <div class="column-2_3 sc_column_item sc_column_item_1 odd first span_2">
                                                        <div class="sc_lessons">

                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        }
                                        else
                                        {
                                            <div style="filter: blur(10px);">
                                                <div style="width:60%">
                                                    @if (Model.cnt != null)
                                                    {
                                                       
                                                        <div class="video-frame">
                                                            <video style="width:100%; height:500px;" controls>
                                                                <source src="~/VideoFileUpload/@Model.cnt.videoname" type="video/mp4" />
                                                            </video>
                                                        </div><br />
                                                        @*<button>Previous</button>
                                                        <button style="margin-left:600px">Next</button>*@
                                                    }
                                                </div>
                                                <div class="columns_wrap sc_columns columns_nofluid sc_columns_count_3">
                                                    <div class="column-2_3 sc_column_item sc_column_item_1 odd first span_2">
                                                        <div class="sc_lessons">

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="button_align" style="margin: 0;position: absolute;top: 50%;-ms-transform: translateY(-50%);transform: translateY(-50%);margin-left:200px; margin-top:150px">
                                                <a href="/Authentication/Login">
                                                    <button>View More</button>
                                                </a>
                                            </div>
                                        }

                                    }
                                    else
                                    {
                                        <div style="width:500px;height:100px;position:relative">
                                            <div style="color: transparent;text-shadow: 0 0 8px #000;filter: blur(10px);">
                                                @if (Model.cnt != null)
                                                {

                                                    

                                                    <div class="video-frame">
                                                        <video style="width:100%; height:500px;" controls>
                                                            <source src="~/VideoFileUpload/@Model.cnt.videoname" type="video/mp4" />
                                                        </video>
                                                    </div><br />
                                                    @*<button>Previous</button>
                                                        <button style="margin-left:600px">Next</button>*@
                                                }
                                            </div>
                                            <div class="columns_wrap sc_columns columns_nofluid sc_columns_count_3">
                                                <div class="column-2_3 sc_column_item sc_column_item_1 odd first span_2">
                                                    <div class="sc_lessons">

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="button_align" style="margin: 0;position: absolute;top: 50%;-ms-transform: translateY(-50%);transform: translateY(-50%);margin-left:200px; margin-top:150px">
                                            <a href="/Authentication/Login">
                                                <button>View More</button>
                                            </a>
                                        </div>
                                    }

                                }

                                else
                                {

                                    <div style="width:62%">
                                        @if (Model.cnt != null)
                                        {
                                         
                                            <div class="video-frame">
                                                <video style="width:100%; height:500px;" controls>
                                                    <source src="~/VideoFileUpload/@Model.cnt.videoname" type="video/mp4" />
                                                </video>
                                            </div><br />
                                            @*<button>Previous</button>
                                            <button style="margin-left:600px">Next</button>*@
                                        }

                                    </div>

                                    <div class="columns_wrap sc_columns columns_nofluid sc_columns_count_3">
                                        <div class="column-2_3 sc_column_item sc_column_item_1 odd first span_2">
                                            <div class="sc_lessons">



                                            </div>
                                        </div>

                                    </div>
                                }
                           

                            </section>

                        </div>
                    </article>
                </div>
                <!-- /Course info section -->
                <div class="sc_gap">
                    <!-- Testimonials section -->
                    <!-- /Testimonials section -->
                    <!-- Pricing section -->

                </div>
            </div>
            <!--Footer-->
            <br />
            <!-- /Widgets Footer -->
            <!-- Contacts Footer -->
            <footer class="contacts_wrap bg_tint_dark contacts_style_dark">
                <div class="content_wrap">
                    <div style="width:100px; height:100px; margin-left:45%">
                        <a href="/CodeHub/Index">
                            <img src="~/assets/media/avatars/Logooe-removebg-preview.png" alt="">
                        </a>
                    </div>
                    <div class="contacts_address">
                        <address class="address_right">
                            Phone: 0321837262<br>

                        </address>
                        <address class="address_left">
                            Gujrat ,PAKISTAN<br>
                            1234 Some St
                        </address>
                    </div>

                </div>
            </footer>
            <!-- /Contacts Footer -->
            <!-- Copyright -->
            <div class="copyright_wrap">
                <div class="content_wrap">
                    <p>© 2015 All Rights Reserved. <a href="#">Terms of use</a> and <a href="#">Privacy Policy</a></p>
                </div>
            </div>
            <!-- /Copyright -->
        </div>
    </div>
    <!-- /Body -->


</body>

</html>
<script>
function myFUNCTION() {
  var x = document.getElementById("myDIV");
  if (x.style.display === "none") {
    x.style.display = "inline";
  } else {
    x.style.display = "none";
  }
}
</script>
<script src="~/assets/plugins/global/plugins.bundle.js"></script>
<script src="~/assets/js/scripts.bundle.js"></script>

